{
  "name": "movingimage/data-provider-vmpro",
  "description": "Dataprovider implementation that retrieves collection data from Video Manager Pro",
  "type": "library",
  "license": "Proprietary",
  "authors": [
    {
      "name": "Ruben Knol",
      "email": "ruben.knol@movingimage.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.mi24.tv/"
    }
  ],
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "movingimage/data-provider": "^1.5.0",
    "movingimage/vmpro-api-client": "^0.5",
    "phpunit/phpunit": "^4.5 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "MovingImage\\DataProvider\\": "lib/DataProvider"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MovingImage\\DataProvider\\Tests\\": "lib/DataProvider"
    }
  }
}
