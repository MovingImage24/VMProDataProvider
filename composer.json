{
    "name": "movingimage/data-provider-vmpro",
    "description": "Dataprovider implementation that retrieves collection data from Video Manager Pro",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Movingimage",
            "email": "developer@movingimage.com"
        }
    ],
    "config": {
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "movingimage/data-provider": "^4.0",
        "movingimage/vmpro-api-client": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MovingImage\\DataProvider\\": "lib/DataProvider"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MovingImage\\DataProvider\\Tests\\": "lib/DataProvider"
        }
    },
    "require-dev": {
        "movingimage/style-checker": "dev-master",
        "phpunit/phpunit": "^8.0"
    }
}
